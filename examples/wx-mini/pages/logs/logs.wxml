<!--logs.wxml-->
<view class="container log-list">
  <map 
  id="map"
  longitude="113.324520"
  latitude="23.099994"
  scale="14"
  subkey="1"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location="true"
  style="width: 100%; height: 300px;"
  subkey="1"
  polygons="{{polygons}}"
  enable-3D="true"
  show-compass="true"
  enable-overlooking="false"
  enable-zoom="true"
  enable-scroll="true"
  enable-rotate="false"
  bindupdated="onupdated"
  bindpoitap="onpoitap"
  bindcallouttap="oncallouttap"
  bindtap="ontap"
  ></map>
</view>
